
<%if false%>
<div class="login-wrapper">
<div class="small-6 small-centered columns">
	<div class="panel">
	<h1 class="text-center">登入</h1>
	<%= form_tag book_login_path  do %>
	  <div class="small-12 columns">
	    <%= label_tag :account, '計中帳號' %>
	    <%= text_field_tag :account %>
	  </div>
	  <div class="small-12 columns">
	    <%= label_tag :password,'計中密碼'%>
	    <%= password_field_tag :password %>
	  </div>
	  <div class="text-center">
	  	<%= submit_tag "登入",class: "button"%>
	  </div>
	<% end %>
	</div>
	<div class="text-center">若有任何問題，請聯絡：<%= Setting.admin_email%></div>
</div>
</div>
<% end %>
